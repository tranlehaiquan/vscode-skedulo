{
  "name": "vscode-skedulo",
  "displayName": "vscode-skedulo",
  "description": "skedulo vscode extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "icons": {
      "skedulo-logo": {
        "description": "Skedulo icon",
        "default": {
          "fontPath": "media/skedulo.woff",
          "fontCharacter": "\\e900"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "skedulo",
          "title": "Skedulo",
          "icon": "media/skedulo.svg"
        }
      ]
    },
    "views": {
      "skedulo": [
        {
          "id": "skedulo-detail",
          "name": "Tenant Detail",
          "icon": "media/dep.svg"
        },
        {
          "id": "skedulo-objects",
          "name": "Objects",
          "icon": "media/skedulo.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "skedulo.refresh",
        "category": "Skedulo",
        "title": "Refresh",
        "icon": "$(extensions-refresh)"
      },
      {
        "command": "skedulo.pullObjects",
        "category": "Skedulo",
        "title": "Pull object",
        "icon": "$(extensions-install-count)"
      },
      {
        "command": "skedulo.login",
        "category": "Skedulo",
        "title": "Login tenant",
        "icon": "$(ports-open-browser-icon)"
      },
      {
        "command": "skedulo.logout",
        "category": "Skedulo",
        "title": "Logout tenant",
        "icon": "$(ports-open-browser-icon)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "skedulo.refresh",
          "when": "view == skedulo-detail",
          "group": "navigation"
        },
        {
          "command": "skedulo.refresh",
          "when": "view == skedulo-objects",
          "group": "navigation"
        },
        {
          "command": "skedulo.pullObjects",
          "when": "view == skedulo-objects",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "skedulo.refresh",
          "when": "view == skedulo-detail && viewItem == object",
          "group": "inline"
        },
        {
          "command": "skedulo.refresh",
          "when": "view == skedulo-detail && viewItem == object",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "^4.14.195",
    "axios": "^1.4.0",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "lodash": "^4.17.21",
    "prettier": "^2.8.8"
  }
}
