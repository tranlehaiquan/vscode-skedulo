{
  "name": "vscode-skedulo",
  "displayName": "vscode-skedulo",
  "description": "skedulo vscode extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "icons": {
      "skedulo-logo": {
        "description": "Skedulo icon",
        "default": {
          "fontPath": "media/skedulo.woff",
          "fontCharacter": "\\e900"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "skedulo",
          "title": "Skedulo",
          "icon": "media/skedulo.svg"
        }
      ]
    },
    "views": {
      "skedulo": [
        {
          "id": "skedulo-detail",
          "name": "Tenant Detail",
          "icon": "media/dep.svg",
          "contextualTitle": "Tenant Detail"
        }
      ]
    },
    "commands": [
      {
        "command": "skedulo.skedulo-detail:refresh",
        "category": "Skedulo",
        "title": "Refresh Tenant Detail"
      },
      {
        "command": "skedulo.showStatus",
        "category": "Skedulo",
        "title": "show status"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.195",
    "axios": "^1.4.0",
    "express": "^4.18.2",
    "lodash": "^4.17.21"
  }
}
